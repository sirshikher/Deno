{
  "$schema": "https://deno.land/x/denon/schema.json",
  "scripts": {
    "start": {
      "cmd": "main.ts",
      "desc": "Run main app",
      "allow": [
        "net",
        "read",
        "write",
        "plugin"
      ],
      "unstable": true
    }
  },
  "logger": {
    "debug": true
  },
  "execute": {
    "ts": ["deno", "run"]
  },
  "watcher": {
    "exts": ["ts", "json", "ini"],
    "skip": ["super_duper_secret/*"]
  }
}